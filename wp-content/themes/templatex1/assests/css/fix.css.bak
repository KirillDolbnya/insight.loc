}

.item_gallery_2{
	width:100%;
}

.projectBottom{
	margin-left:-3%;
	margin-right:-3%;
}

.item_gallery_3_wrap{
	width:33.33333333333333%;
	padding-left:3%;
	box-sizing:border-box;
	padding-right:3%;
}

.item_gallery_3_wrap img{
	width:100%;
	object-fit:cover;
	height:400px;
}

}


