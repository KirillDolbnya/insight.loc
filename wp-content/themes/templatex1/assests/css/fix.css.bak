}

.left_object_fix .leftscreen{
	width:100%;
}

.object_menu_fix{
	max-width:160px;
	position:absolute;
	right:0px;
	height:100%;
}

.object_menu_fix .proMenu{
	padding-left:0px;
}

.left_object_fix .secondProj {
  position: relative;
  left: 0px;
}

.left_object_fix2{
	box-sizing:border-box;
	width:55.61%;
	padding-left:17%;
	padding-right: 183px;
}

.left_object_fix2 img.fotoAdaptiv{
	height:434px;
	width:100%;
}

.left_object_fix2 .leftscreen{
	width:100%;
	box-sizing:border-box;
}

.left_object_fix2 .projectFour{
	margin-left:0px;
