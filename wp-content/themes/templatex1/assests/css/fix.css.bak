.wpcf7-not-valid-tip{
	display:none !important;
}

.wpcf7 form .wpcf7-response-output{
	margin-top:0 !important;
	display:none !important;
}

.wpcf7-not-valid{
	border:1px solid red !important;
}

ul.nomargin{
	margin:0px;
	padding:0px;
}

.breadcrumbs a{
	text-decoration:none;
	color:inherit;
}

.separator_bread{
	margin-left:3px;
	margin-right:3px;
}

.leftscreen.leftscreen_visible{
	overflow:visible;
}


.projectsAdapt{
	display:block;
	text-decoration:none !important;
	color:inherit !important;
}

@media (min-width: 768px){

.projectBig{
	min-height:696px;
	object-fit:cover;
}

.leftscreen{
	overflow:hidden;
}

.projectSmall_img {
	max-width:251px;
	max-height:251px;
	object-fit:cover;
}

.projectMiddle_img {
	max-width:251px;
	max-height:351px;
	object-fit:cover;
}


.projectsAdapt{
	text-align:left;
	width:100%;
}

.fotoAdaptiv_1{
	max-width:449px;
}

.fotoAdaptiv_1 img.fotoAdaptiv{
	height:346px;
	width:100%;
	object-fit:cover;
}

.fotoAdaptiv_3 {
	max-width:651px;
}

.fotoAdaptiv_3 img.fotoAdaptiv{
	height:434px;
	object-fit:cover;
	width:100%;
}

.fotoAdaptiv_2{
}

.fotoAdaptiv_2 img.fotoAdaptiv{
	height:408px;
	object-fit:cover;
	width:100%;
}

.left_object_fix{
	width:55.61%;
	padding-right:183px;
	position:relative;
	padding-left:2%;
