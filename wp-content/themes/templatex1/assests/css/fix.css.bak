.projectBottom{
	flex-wrap:wrap;
}

.projectBottom .projectAdaptiv{
	margin-bottom:30px;
}

.rightscreen_item_gallery_1{
	box-sizing:border-box;
	padding-right: 11%;
}

.rightscreen_item_gallery_1 img{
